//ALEXDEQ  JOB ...
//*
//* ASSEMBLE EVALBLOK S/ROUTINE
//*
//* 03/2001 AXB
// JCLLIB ORDER=ALEX.ASM                              <--- CHANGE THAT
//RLTSUB   EXEC LGNASML,
//         AMEMB=RLTSUB,
//         SRCDSN=ALEX.ASM,
//         ASMPARM='NODECK,OBJECT,TERM,RENT',
//         LNKPARM='XREF,MAP,LIST,RENT',
//         LOADLIB=ALEX.LOAD,                         <--- CHANGE THAT
//         MAC1=SYS1.MACLIB,
//         MAC2=SYS1.MODGEN,
//         LNK1=SYS1.CSSLIB
//L.SYSLIN DD   *
 INCLUDE OBJECT
 ENTRY RLTSUB
 NAME RLTSUB(R)
//*
//* ASSEMBLE RXDEQ    REXX FUNCTION
//*   AND GENERATE EXECUTABLE MODULE.
//* 04/2001 AXB
//RXDEQ    EXEC LGNASML,
//         AMEMB=RXDEQ,
//         SRCDSN=ALEX.ASM,                           <--- CHANGE THAT
//         ASMPARM='NODECK,OBJECT,TERM,RENT',
//         LNKPARM='XREF,MAP,LIST,RENT',
//         LOADLIB=ALEX.LOAD,                         <--- CHANGE THAT
//         MAC1=SYS1.MACLIB,
//         MAC2=SYS1.MODGEN,
//         LNK1=SYS1.CSSLIB
//L.SYSLIN DD   *
 INCLUDE OBJECT
 INCLUDE SYSLMOD(RLTSUB)
 ORDER   RXDEQ
 ENTRY RXDEQ
 NAME RXDEQ(R)
