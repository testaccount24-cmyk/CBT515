//ALEXSLEE JOB ...
//*
//* ASSEMBLE EVALBLOK S/ROUTINE FOR ISPF LOGON PROC PROGRAM
//*
//* 01/2001 AXB
// JCLLIB ORDER=ALEX.ASM                         <--- CHANGE THAT
//RLTSUB   EXEC LGNASML,
//         AMEMB=RLTSUB,
//         SRCDSN=ALEX.ASM,                      <--- CHANGE THAT
//         ASMPARM='NODECK,OBJECT,TERM,RENT',
//         LNKPARM='XREF,MAP,LIST,RENT',
//         LOADLIB=ALEX.LOAD,                    <--- CHANGE THAT
//         MAC1=SYS1.MACLIB,
//         MAC2=SYS1.MODGEN,
//         LNK1=SYS1.CSSLIB
//L.SYSLIN DD   *
 INCLUDE OBJECT
 ENTRY RLTSUB
 NAME RLTSUB(R)
//*
//* ASSEMBLE SLEEP REXX FUNCTION
//*   AND GENERATE EXECUTABLE MODULE.
//* 02/2001 AXB
//SLEEPM   EXEC LGNASML,
//         AMEMB=SLEEP,
//         SRCDSN=ALEX.ASM,                      <--- CHANGE THAT
//         ASMPARM='NODECK,OBJECT,TERM,RENT',
//         LNKPARM='XREF,MAP,LIST,RENT',
//         LOADLIB=ALEX.LOAD,                    <--- CHANGE THAT
//         MAC1=SYS1.MACLIB,
//         MAC2=SYS1.MODGEN,
//         LNK1=SYS1.CSSLIB
//L.SYSLIN DD   *
 INCLUDE OBJECT
 INCLUDE SYSLMOD(RLTSUB)
 ORDER   SLEEP
 ENTRY SLEEP
 NAME SLEEP(R)
