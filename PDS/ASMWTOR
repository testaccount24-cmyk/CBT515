//ALEXWTO  JOB ...
//*
//* ASSEMBLE EVALBLOK S/ROUTINE
//*
//* 03/2001 AXB
// JCLLIB ORDER=ALEX.ASM                          <--- CHANGE THAT
//RLTSUB   EXEC LGNASML,
//         AMEMB=RLTSUB,
//         SRCDSN=ALEX.ASM,                       <--- CHANGE THAT
//         ASMPARM='NODECK,OBJECT,TERM,RENT',
//         LNKPARM='XREF,MAP,LIST,RENT',
//         LOADLIB=ALEX.LOAD,                     <--- CHANGE THAT
//         MAC1=SYS1.MACLIB,
//         MAC2=SYS1.MODGEN,
//         LNK1=SYS1.CSSLIB
//L.SYSLIN DD   *
 INCLUDE OBJECT
 ENTRY RLTSUB
 NAME RLTSUB(R)
//*
//* ASSEMBLE RXWTOR   REXX FUNCTION
//*   AND GENERATE EXECUTABLE MODULE.
//* 02/2001 AXB
//RXWTOR   EXEC LGNASML,
//         AMEMB=RXWTOR,
//         SRCDSN=ALEX.ASM,                       <--- CHANGE THAT
//         ASMPARM='NODECK,OBJECT,TERM,RENT',
//         LNKPARM='XREF,MAP,LIST,RENT',
//         LOADLIB=ALEX.LOAD,                     <--- CHANGE THAT
//         MAC1=SYS1.MACLIB,
//         MAC2=SYS1.MODGEN,
//         LNK1=SYS1.CSSLIB
//L.SYSLIN DD   *
 INCLUDE OBJECT
 INCLUDE SYSLMOD(RLTSUB)
 ORDER   RXWTOR
 ENTRY RXWTOR
 NAME RXWTOR(R)
