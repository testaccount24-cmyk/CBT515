//ALEXSPFP JOB ...
//*
//* ASSEMBLE EVALBLOK S/ROUTINE FOR ISPF LOGON PROC PROGRAM
//*
//* 01/2001 AXB
// JCLLIB ORDER=ALEX.ASM                            <--- CHANGE THAT
//RLTSUB   EXEC LGNASML,
//         AMEMB=RLTSUB,
//         SRCDSN=ALEX.ASM,                         <--- CHANGE THAT
//         ASMPARM='NODECK,OBJECT,TERM,RENT',
//         LNKPARM='XREF,MAP,LIST,RENT',
//         LOADLIB=ALEX.LOAD,                       <--- CHANGE THAT
//         MAC1=SYS1.MACLIB,
//         MAC2=SYS1.MODGEN,
//         LNK1=SYS1.CSSLIB
//L.SYSLIN DD   *
 INCLUDE OBJECT
 ENTRY RLTSUB
 NAME RLTSUB(R)
//*
//* ASSEMBLE UDEF TABLE FOR ISPF LOGON PROC - EXCEPTIONS
//*
//LGNUDEFE EXEC LGNASML,
//         AMEMB=LGNUDEFE,
//         SRCDSN=ALEX.ASM,                         <--- CHANGE THAT
//         ASMPARM='NODECK,OBJECT,TERM,RENT',
//         LNKPARM='XREF,MAP,LIST,RENT',
//         LOADLIB=ALEX.LOAD,                       <--- CHANGE THAT
//         MAC1=SYS1.MACLIB,
//         MAC2=SYS1.MODGEN,
//         LNK1=SYS1.CSSLIB
//L.SYSLIN DD   *
 INCLUDE OBJECT
 ENTRY LGNUDEFE
 NAME LGNUDEFE(R)
//*
//* ASSEMBLE UDEF TABLE FOR ISPF LOGON PROC - DEFAULTS
//*
//LGNUDEFD EXEC LGNASML,
//         AMEMB=LGNUDEFD,
//         SRCDSN=ALEX.ASM,                           <--- CHANGE THAT
//         ASMPARM='NODECK,OBJECT,TERM,RENT',
//         LNKPARM='XREF,MAP,LIST,RENT',
//         LOADLIB=ALEX.LOAD,                         <--- CHANGE THAT
//         MAC1=SYS1.MACLIB,
//         MAC2=SYS1.MODGEN,
//         LNK1=SYS1.CSSLIB
//L.SYSLIN DD   *
 INCLUDE OBJECT
 ENTRY LGNUDEFD
 NAME LGNUDEFD(R)
//*
//* ASSEMBLE REXX FUNCTION TO USE ISPF LOGON PROC TABLE
//*   AND GENERATE EXECUTABLE MODULE.
//* 01/2001 AXB
//LGNSPF   EXEC LGNASML,
//         AMEMB=LGNSPF,
//         SRCDSN=ALEX.ASM,                           <--- CHANGE THAT
//         ASMPARM='NODECK,OBJECT,TERM,RENT',
//         LNKPARM='XREF,MAP,LIST,RENT',
//         LOADLIB=YOUR.LINKLIB,                      <--- CHANGE THAT
//*        LOADLIB=CPAC.LINKLIB,                         2.9
//         MAC1=SYS1.MACLIB,
//         MAC2=SYS1.MODGEN,
//         LNK1=SYS1.CSSLIB
//L.TMPLMOD DD DISP=SHR,DSN=ALEX.LOAD                 <--- CHANGE THAT
//L.SYSLIN DD   *
 INCLUDE OBJECT
 INCLUDE TMPLMOD(LGNUDEFE)
 INCLUDE TMPLMOD(LGNUDEFD)
 INCLUDE TMPLMOD(RLTSUB)
 ORDER   LGNSPF
 ENTRY LGNSPF
 NAME LGNSPF(R)
